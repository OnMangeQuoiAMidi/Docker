{
  "name": "on_mange_quoi_a_midi",
  "version": "0.0.1",
  "description": "On Mange Quoi A Midi, a symfony application made by ECV Digital Master Development",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build:js:dev": "browserify src/AppBundle/Resources/public/js/main.js --debug -o src/AppBundle/Resources/public/js/bundle.js -t rollupify && npm run lint:js",
    "build:js:prod": "browserify src/AppBundle/Resources/public/js/main.js -o src/AppBundle/Resources/public/js/bundle.js -t rollupify",
    "watch:js": "onchange 'src/AppBundle/Resources/public/js/**/*.js' -e 'src/AppBundle/Resources/public/js/bundle.js' -- npm run build:js:dev",
    "uglify:js": "uglifyjs src/AppBundle/Resources/public/js/bundle.js -m -o src/AppBundle/Resources/public/js/bundle.js",
    "lint:js": "eslint src/AppBundle/Resources/public/js; exit 0",
    "build:css:dev": "node-sass --source-map true --include-path src/AppBundle/Resources/public/scss --include-path node_modules/material-design-lite/src -o src/AppBundle/Resources/public/css src/AppBundle/Resources/public/scss/main.scss",
    "build:css:prod": "node-sass --output-style compressed --include-path src/AppBundle/Resources/public/scss --include-path node_modules/material-design-lite/src -o src/AppBundle/Resources/public/css src/AppBundle/Resources/public/scss/main.scss",
    "watch:css": "onchange 'src/AppBundle/Resources/public/scss/**/*.scss' -- npm run build:css:dev",
    "build:dev": "npm run build:js:dev && npm run build:css:dev",
    "build:prod": "npm run build:js:prod && npm run uglify:js && npm run build:css:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecvdbdx1617/OnMangeQuoiAMidi.git"
  },
  "author": "ECV Digital",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ecvdbdx1617/OnMangeQuoiAMidi/issues"
  },
  "homepage": "https://github.com/ecvdbdx1617/OnMangeQuoiAMidi#readme",
  "dependencies": {
    "jquery": "^3.1.1",
    "material-design-lite": "^1.2.1"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "eslint": "^3.11.1",
    "eslint-config-google": "^0.7.1",
    "node-sass": "^3.13.0",
    "onchange": "^3.2.0",
    "rollupify": "^0.3.5",
    "uglify-js": "^2.7.5"
  }
}
